
{
  "name": "ai_model_config",
  "title": "AI模型配置",
  "schemas": [
    {
      "name": "model_name",
      "title": "模型名称",
      "type": "String"
    },
    {
      "name": "model_type",
      "title": "模型类型",
      "type": "String"
    },
    {
      "name": "model_version",
      "title": "模型版本",
      "type": "String"
    },
    {
      "name": "api_key",
      "title": "API密钥",
      "type": "String"
    },
    {
      "name": "api_secret",
      "title": "API密钥",
      "type": "String"
    },
    {
