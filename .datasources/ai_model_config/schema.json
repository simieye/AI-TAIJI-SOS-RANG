      "title": "自定义请求头",
      "type": "Object",
      "items": {
        "type": "String"
      }
    },
    {
      "name": "encryption_enabled",
      "title": "启用加密",
      "type": "Boolean"
    },
    {
      "name": "logging_enabled",
      "title": "启用日志",
      "type": "Boolean"
    },
    {
      "name": "cache_enabled",
      "title": "启用缓存",
      "type": "Boolean"
    },
    {
      "name": "cache_ttl",
      "title": "缓存过期时间(秒)",
      "type": "Number"
    },
    {
      "name": "tags",
      "title": "标签",
      "type": "Array",
      "items": {
        "type": "String"
      }
    },
    {
      "name": "notes",
      "title": "备注信息",
      "type": "MultiLineString"
    }
